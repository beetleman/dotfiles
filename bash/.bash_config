# User specific aliases and functions

# aliases config:
[[ -f ~/.bash_aliases ]] && . ~/.bash_aliases

# promot config:
[[ -f ~/.bash_prompt ]] && . ~/.bash_prompt


# general path:
PATH=$PATH:$HOME/.local/bin:$HOME/bin:$HOME/node_modules/.bin

# andorind sdk path:
PATH=$PATH:$HOME/opt/android/sdk/tools/:$HOME/opt/android/sdk/platform-tools/

export PATH

export EDITOR=zile
